domain idltest.services

data Success {
  greeting: str
}

data Error {
  message: str
}

mixin Request {
  firstName: str
  secondName: str
}

service TestService {
  def greetSimple(firstName: str, secondName: str) => (a: str)

  def greetComplex
    (
      + Request
      region: str,
      age: int8
    )
      =>
    (
        +Success
    )

  def greetAlgebraic(firstName: str, secondName: str) => ( Success | Error )
  def greetAlgebraic2(firstName: str, secondName: str) => (
     Success
   | Error
  )
}
