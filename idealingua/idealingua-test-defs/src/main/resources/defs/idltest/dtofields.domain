domain idltest.dtofields

mixin IntPair {
  x: i32
  y: i32
}

mixin WHPair {
    w: i32
    h: i32
}

mixin Metadata {
  id: str
  name: str
}

mixin PointLike {
  & Metadata
  + IntPair
}

data Point {
  & Metadata
  & WHPair
  + IntPair
  ownfield: str
  name: str
}

mixin NullableContent {
    a: i32
}

data NullableObj {
    &NullableContent
}

data OptionalObj {
    no: opt[NullableObj]
}

alias NullObj = NullableObj

data ListObj {
    all: list[NullObj]
}